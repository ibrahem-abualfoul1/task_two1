<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <!--     Fonts and icons     -->
    <link rel="stylesheet" type="text/css" href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700|Roboto+Slab:400,700|Material+Icons" />
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
    <!-- Material Kit CSS -->
    <link href="/assets/css/material-dashboard.css" rel="stylesheet" />
</head>
<body>
    
    <div class="wrapper ">
        <div class="sidebar" data-color="purple" data-background-color="white">
            <!--
                Tip 1: You can change the color of the sidebar using: data-color="purple | azure | green | orange | danger"

                Tip 2: you can also add an image using data-image tag
            -->
            <div class="logo">
                <a  class="simple-text logo-mini">
                    Hello admin
                </a>
                
            </div>
            <div class="sidebar-wrapper">
                <ul class="nav">
                    <li class="nav-item active  ">
                        <a class="nav-link" href="/Accounts/Index">
                            <i class="material-icons">dashboard</i>
                            <p>Accounts</p>
                        </a>
                    </li>
                    <li class="nav-item active  ">
                        <a class="nav-link" href="/Accounts/Index">
                            <i class="material-icons">dashboard</i>
                            <p>Accounts</p>
                        </a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/Categories/Index">
                            <i class="material-icons">dashboard</i>
                            <p>Catigories</p>
                        </a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/MangePages/Index">
                            <i class="material-icons">dashboard</i>
                            <p>MangePages</p>
                        </a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/Prodact/Index">
                            <i class="material-icons">dashboard</i>
                            <p>Productes</p>
                        </a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/Testimonials/Index">
                            <i class="material-icons">dashboard</i>
                            <p>Testimonials</p>
                        </a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/Transtions/Index">
                            <i class="material-icons">dashboard</i>
                            <p>Transtions</p>
                        </a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/Messages/Index">
                            <i class="material-icons">dashboard</i>
                            <p>Messages</p>
                        </a>
                    </li>
                    <li class="nav-item active">
                        <a class="nav-link" href="/Home/Index">
                            <i class="material-icons">dashboard</i>
                            <p>Logout</p>
                        </a>
                    </li>
                    <!-- your sidebar here/Messages/Index -->
                </ul>
            </div>
        </div>
        <div class="main-panel">
            <!-- Navbar -->
            <nav class="navbar navbar-expand-lg navbar-transparent navbar-absolute fixed-top ">
                <div class="container-fluid">
                    <div class="navbar-wrapper">
                        <a class="navbar-brand" href="javascript:;">Dashboard</a>
                    </div>

                    <button class="navbar-toggler" type="button" data-toggle="collapse" aria-controls="navigation-index" aria-expanded="false" aria-label="Toggle navigation">
                        <span class="sr-only">Toggle navigation</span>
                        <span class="navbar-toggler-icon icon-bar"></span>
                        <span class="navbar-toggler-icon icon-bar"></span>
                        <span class="navbar-toggler-icon icon-bar"></span>
                    </button>
                    <div class="collapse navbar-collapse justify-content-end">
                        <ul class="navbar-nav">
                            <li class="nav-item">
                                <a class="nav-link" href="javascript:;">
                                    <i class="material-icons">notifications</i> Notifications
                                </a>
                            </li>
                            <!-- your navbar here -->
                        </ul>
                    </div>
                </div>
            </nav>
            <!-- End Navbar -->
            <div class="content">
                <div class="container-fluid">
                    <!-- your content here -->
                    <div>
                        @RenderBody()
                    </div>
                </div>
            </div>

            <footer class="footer">
                <div class="container-fluid">
                    <nav class="float-left">
                        <ul>
                            <li>
                                <a href="https://www.creative-tim.com">
                                    Creative Tim
                                </a>
                            </li>
                        </ul>
                    </nav>
                    <div class="copyright float-right">
                        &copy;
                        <script>
                            document.write(new Date().getFullYear())
                        </script>, made with <i class="material-icons">favorite</i> by
                        <a href="https://www.creative-tim.com" target="_blank">Creative Tim</a> for a better web.
                    </div>
                    <!-- your footer here -->
                </div>
            </footer>
</body>
</html>
